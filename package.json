{
  "name": "AwesomeReactBoilerplate",
  "version": "0.1.2",
  "scripts": {
    "start": "babel-node ./src/www_frontend/.bin/client/main.js",
    "graphiql": "babel-node ./src/www_frontend/.bin/graphiql/main.js",
    "relay": "relay-compiler --src ./src/www_frontend --schema ./schema.graphql",
    "build": "webpack -p --config ./__webpack__/frontend/config.prod.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^cores(.*)$": "<rootDir>/src/www_cores$1",
      "^helpers$": "<rootDir>/src/helpers",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(png|jpg|jpeg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "graphql": "^0.9.3",
    "lodash": "^4.16.6",
    "moment": "^2.15.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-loading": "0.0.9",
    "react-logger": "^1.1.0",
    "react-paginate": "^4.1.1",
    "react-redux": "^5.0.4",
    "react-relay": "^1.0.0-rc.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-logger": "^2.10.2",
    "redux-modal": "^1.3.1",
    "redux-saga": "^0.14.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.17.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-relay": "^1.0.1-rc.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.11.0",
    "cleave.js": "^0.7.14",
    "css-loader": "^0.24.0",
    "enzyme": "^2.7.0",
    "express": "^4.15.0",
    "express-graphql": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "graphiql": "^0.9.3",
    "history": "^4.5.1",
    "html-webpack-plugin": "^2.24.1",
    "isomorphic-relay": "^0.7.4",
    "jest": "^18.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.9.3",
    "react-addons-create-fragment": "^15.4.2",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.1",
    "redux-devtools": "^3.3.1",
    "redux-devtools-chart-monitor": "^1.4.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-slider-monitor": "^1.0.7",
    "relay-compiler": "^1.0.0-rc.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-md5-hash": "0.0.5"
  }
}
