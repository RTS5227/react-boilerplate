{
  "name": "AwesomeReactBoilerplate",
  "version": "0.1.2",
  "scripts": {
    "start": "npm run startClient | npm run startServer",
    "startClient": "babel-node ./__webpack__/frontend/client/main.js",
    "startServer": "babel-node ./__webpack__/frontend/server/main.js",
    "dev": "webpack --config ./__webpack__/frontend/client/config.dev.js",
    "build": "webpack -p --config ./__webpack__/frontend/client/config.prod.js",
    "buildServer": "webpack -p --config ./__webpack__/frontend/server/webpack.config.js",
    "graphiql": "babel-node ./src/www_frontend/.scripts/graphiql/main.js",
    "relay": "relay-compiler --src ./src/www_frontend --schema ./schema.graphql",
    "updateSchema": "babel-node ./data/updateSchema.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^cores(.*)$": "<rootDir>/src/www_cores$1",
      "^helpers$": "<rootDir>/src/helpers",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(png|jpg|jpeg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "babel-loader": "^6.4.1",
    "babel-plugin-relay": "^1.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "graphiql": "^0.9.3",
    "graphql": "^0.9.3",
    "history": "^4.6.2",
    "html-webpack-plugin": "^2.28.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-relay": "^0.7.4",
    "lodash": "^4.16.6",
    "moment": "^2.15.1",
    "prop-types": "^15.5.8",
    "react": "^15.6.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-loading": "0.0.9",
    "react-paginate": "^4.4.3",
    "react-redux": "^5.0.5",
    "react-relay": "^1.0.0",
    "redux": "^3.7.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-form": "^6.8.0",
    "redux-logger": "^2.10.2",
    "redux-modal": "^1.4.1",
    "redux-saga": "^0.14.8",
    "relay-compiler": "^1.0.0",
    "relay-runtime": "^1.0.0",
    "sass-loader": "^4.1.1",
    "universal-router": "^3.2.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.5.0",
    "webpack-isomorphic-tools": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.17.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-relay": "^1.0.1-rc.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "cleave.js": "^0.7.14",
    "compression": "^1.6.2",
    "css-loader": "^0.24.0",
    "enzyme": "^2.7.0",
    "express": "^4.15.0",
    "express-graphql": "^0.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "graphiql": "^0.9.3",
    "history": "^4.5.1",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^18.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.9.3",
    "piping": "^1.0.0-rc.4",
    "react-addons-create-fragment": "^15.4.2",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.4.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "relay-compiler": "^1.0.0-rc.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^3.0.0",
