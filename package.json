{
  "name": "AwesomeReact",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "wp:start": "node ./__webpack__/admin/server.js",
    "wp:liveload": "./node_modules/.bin/webpack-dev-server -d --colors --config ./__webpack__/admin/config.dev.js",
    "wp:dev": "./node_modules/.bin/webpack-dev-server --hot --config ./__webpack__/admin/config.dev.js --colors",
    "wp:build": "BUILD_DIR=${BUILD_DIR} ./node_modules/.bin/webpack -p --config ./__webpack__/admin/config.prod.js",
    "wp:start_customer": "node ./__webpack__/customer/server.js",
    "wp:liveload_customer": "./node_modules/.bin/webpack-dev-server -d --colors --config ./__webpack__/customer/config.dev.js",
    "wp:dev_customer": "./node_modules/.bin/webpack-dev-server --hot --config ./__webpack__/customer/config.dev.js --colors",
    "wp:build_customer": "BUILD_DIR=${BUILD_DIR} ./node_modules/.bin/webpack -p --config ./__webpack__/customer/config.prod.js",
    "wp:analyzer": "./node_modules/.bin/webpack --json --config ./__webpack__/admin/config.dev.js | webpack-bundle-size-analyzer",
    "test": "jest",
    "test:liveload": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^cores(.*)$": "<rootDir>/src/www_cores$1",
      "^helpers$": "<rootDir>/src/helpers",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(png|jpg|jpeg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "cleave.js": "^0.7.14",
    "event-stream": "^3.3.4",
    "fbjs": "^0.8.5",
    "humps": "^2.0.0",
    "lodash": "^4.16.6",
    "moment": "^2.15.1",
    "normalizr": "^2.2.1",
    "numeral": "^1.5.6",
    "react": "^15.4.2",
    "react-addons-create-fragment": "^15.4.2",
    "react-addons-perf": "^15.4.2",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.2",
    "react-loading": "0.0.9",
    "react-logger": "^1.1.0",
    "react-paginate": "^4.1.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-saga": "^0.2.4",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-modal": "^1.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "css-loader": "^0.24.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.9.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^18.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.9.3",
    "react-addons-test-utils": "^15.4.1",
    "react-datatable": "0.0.31",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.1",
    "redux-devtools": "^3.3.1",
    "redux-devtools-chart-monitor": "^1.4.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-slider-monitor": "^1.0.7",
    "redux-test-recorder": "^1.3.1",
    "redux-test-recorder-react": "^1.0.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-md5-hash": "0.0.5"
  }
