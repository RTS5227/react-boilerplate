{
  "name": "AwesomeWeb",
  "version": "0.1.0",
  "main": "index.js",
  "config": {
    "appFrontendHost": "0.0.0.0",
    "appFrontendPort": 4040,
    "graphqlHost": "0.0.0.0",
    "graphqlPort": 3000
  },
  "scripts": {
    "server:start": "babel-node ./__server__/frontend/index.js",
    "wp:start": "babel-node ./src/www_frontend/__command__/webpack/server.js",
    "wp:update": "babel-node ./__tools__/updateSchema.js",
    "wp:build": "BUILD_DIR=${BUILD_DIR} ./node_modules/.bin/webpack -p --config ./__webpack__/frontend/config.prod.js",
    "wp:analyzer": "./node_modules/.bin/webpack --json --config ./__webpack__/frontend/config.dev.js | webpack-bundle-size-analyzer",
    "test": "jest",
    "test:liveload": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^cores(.*)$": "<rootDir>/src/www_cores$1",
      "^helpers$": "<rootDir>/src/helpers",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(png|jpg|jpeg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "cleave.js": "^0.7.14",
    "express": "^4.15.0",
    "express-graphql": "^0.6.3",
    "graphql": "^0.9.1",
    "graphql-relay": "^0.5.1",
    "graphql-sequelize": "^5.2.1",
    "history": "^4.5.1",
    "humps": "^2.0.0",
    "isomorphic-relay": "^0.7.4",
    "lodash": "^4.16.6",
    "moment": "^2.15.1",
    "normalizr": "^3.2.2",
    "react": "^15.4.2",
    "react-addons-create-fragment": "^15.4.2",
    "react-addons-perf": "^15.4.2",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-loading": "0.0.9",
    "react-logger": "^1.1.0",
    "react-paginate": "^4.1.1",
    "react-relay": "^0.10.0",
    "react-router": "^3.0.2",
    "react-router-relay": "^0.13.5"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.11.0",
    "css-loader": "^0.24.0",
    "enzyme": "^2.7.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^18.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.9.3",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-md5-hash": "0.0.5"
  }
}
